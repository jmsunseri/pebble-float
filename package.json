{
  "name": "pebble-float",
  "author": "Justin Sunseri",
  "version": "1.0.0",
  "keywords": [
    "pebble-watchface"
  ],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4",
    "pebble-events": "^1.2.0"
  },
  "pebble": {
    "displayName": "Float",
    "uuid": "13d081cd-2f21-4ff7-9c01-9c41a14c18ef",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt"
    ],
    "watchapp": {
      "watchface": true
    },
    "capabilities": [
      "health",
      "configurable"
    ],
    "messageKeys": [
      "dummy",
      "TimeColor",
      "DateColor",
      "WeatherColor",
      "BackgroundColor",
      "HealthColor",
      "IsTempMetricUnits",
      "IsDistanceMetricUnits",
      "IsHealthEnabled",
      "IsWeatherEnabled",
      "IsDateEnabled",
      "IsBatteryEnabled",
      "Request",
      "Reply",
      "TempK"
    ],
    "resources": {
      "media": [
        {
          "characterRegex": "[0-9 ]",
          "type": "font",
          "name": "DECIMAL_MEDIUM_66",
          "file": "fonts/Justin-Decimal-Medium.ttf"
        },
        {
          "characterRegex": "[:0-9 ]",
          "type": "font",
          "name": "DECIMAL_MEDIUM_32",
          "file": "fonts/Justin-Decimal-Medium.ttf"
        },
        {
          "characterRegex": "[:0-9,a-zA-ZÂ° .]",
          "type": "font",
          "name": "DECIMAL_MEDIUM_16",
          "file": "fonts/Justin-Decimal-Medium.ttf"
        }
      ]
    }
  }
}